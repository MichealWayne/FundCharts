<view class="fundCharts-demo">
  <View>
    <!-- chart 1 -->
    <view class="u-h3">1.单条折线图（带交互）</view>
    <view class='s-bg_linear2'><view class="u-label">
        time: <text>{{line1Time}}</text> 
        sy: <text>{{line1Sy}}</text>
    </view>
    <canvas
      class="u-canvas"
      canvas-id="chartline1"
      bindtouchstart="chart1Touchstart"
	    bindtouchmove="chart1Touchmove"
    ></canvas></view>

    <!-- chart 2 -->
    <view class="u-h3">2.多条折线图（带交互）</view>
    <view class="u-label">
        line1 time: <text>{{line2Time1}}</text> 
        sy: <text>{{line2Sy1}}</text>
    </view>
    <view class="u-label">
        line2 time: <text>{{line2Time2}}</text> 
        sy: <text>{{line2Sy2}}</text>
    </view>
    <view class="u-label">
        line3 time: <text>{{line2Time3}}</text> 
        sy: <text>{{line2Sy3}}</text>
    </view>
    <canvas
      class="u-canvas"
      canvas-id="chartline2"
      bindtouchstart="chart2Touchstart"
	    bindtouchmove="chart2Touchmove"
    ></canvas>

    <!-- chart 3 -->
    <view class="u-h3">3.面积图</view>
    <canvas
      class="u-canvas"
      canvas-id="chartline3"
      bindtouchstart="chart3Touchstart"
	    bindtouchmove="chart3Touchmove"
    ></canvas>

    <!-- chart 4 -->
    <view class="u-h3">4.曲线图</view>
    <view class='s-bg_linear'><canvas
      class="u-canvas"
      canvas-id="chartline4"
    ></canvas></view>

    <!-- chart 5 -->
    <view class="u-h3">5.带数据点</view>
    <canvas
      class="u-canvas"
      canvas-id="chartline5"
    ></canvas>

  </View>
</view>