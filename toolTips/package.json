{
  "name": "fundcharts-tooltips",
  "version": "0.0.1",
  "description": "FundCharts Tooltips",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --env.config production"
  },
  "author": "Micheal Wayne",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-enough": "^0.3.0",
    "eslint-loader": "^2.1.1",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true
        }
      ],
      "stage-2"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "enough",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "array-callback-return": [
        0
      ],
      "no-var": [
        0
      ],
      "no-trailing-spaces": [
        0
      ],
      "space-before-blocks": [
        0
      ],
      "prefer-const": [
        0
      ],
      "prefer-arrow-callback": [
        0
      ],
      "new-cap": [
        0
      ],
      "quote-props": [
        0
      ],
      "valid-jsdoc": [
        0
      ],
      "prefer-spread": [
        0
      ],
      "keyword-space": [
        0
      ],
      "dot-notation": [
        0
      ],
      "prefer-rest-params": [
        0
      ],
      "padded-blocks": [
        0
      ],
      "no-undef": [
        0
      ],
      "no-multi-spaces": [
        0
      ],
      "no-unused-expressions": [
        0
      ],
      "no-unreachable": [
        0
      ],
      "no-extra-semi": [
        0
      ],
      "no-empty": [
        0
      ],
      "no-array-constructor": [
        0
      ],
      "quotes": [
        0
      ],
      "no-extra-parens": [
        0
      ],
      "space-in-parens": [
        0
      ],
      "wrap-iife": [
        0
      ],
      "object-shorthand": [
        0
      ],
      "func-call-spacing": [
        0
      ],
      "eqeqeq": [
        0
      ],
      "indent": [
        0
      ],
      "semi": [
        0
      ],
      "no-unused-vars": [
        2,
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "no-mixed-spaces-and-tabs": [
        0
      ],
      "no-console": [
        0
      ],
      "space-before-function-paren": [
        0
      ],
      "spaced-comment": [
        0
      ],
      "no-alert": [
        0
      ],
      "linebreak-style": [
        "off",
        "windows"
      ]
    }
  }
}
